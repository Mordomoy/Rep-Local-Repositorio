matriz = [
   [1,2,3],
   [4,5,6],
   [7,8,9]
]
numero = matriz [2] [1]
#print(f"numero = {numero}")

def exibir (matriz):
    print("  0 1 2")
    for linha in range(3):
        print(linha, end=' ')
        for coluna in range (3):
            print(matriz[linha][coluna], end=' ') 
        print()

exibir (matriz)   